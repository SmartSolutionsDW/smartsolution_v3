<p id="notice"><%= notice %></p>

<h1>Listing Tipo Productos</h1>

<table>
  <thead>
    <tr>
      <th>Co tipoproducto</th>
      <th>No cotipoproducto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_productos.each do |tipo_producto| %>
      <tr>
        <td><%= tipo_producto.Co_TipoProducto %></td>
        <td><%= tipo_producto.No_CoTipoProducto %></td>
        <td><%= link_to 'Show', tipo_producto %></td>
        <td><%= link_to 'Edit', edit_tipo_producto_path(tipo_producto) %></td>
        <td><%= link_to 'Destroy', tipo_producto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo producto', new_tipo_producto_path %>
