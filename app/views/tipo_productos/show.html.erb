<%- model_class = TipoProducto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:Co_TipoProducto) %>:</strong></dt>
  <dd><%= @tipo_producto.Co_TipoProducto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:co_coTipoProducto) %>:</strong></dt>
  <dd><%= @tipo_producto.co_coTipoProducto %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              tipo_productos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tipo_producto_path(@tipo_producto), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tipo_producto_path(@tipo_producto),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
