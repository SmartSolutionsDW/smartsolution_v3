<p id="notice"><%= notice %></p>

<h1>Listing Salas</h1>

<table>
  <thead>
    <tr>
      <th>Co sala</th>
      <th>Co local</th>
      <th>No sala</th>
      <th>Qt capacidad</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @salas.each do |sala| %>
      <tr>
        <td><%= sala.co_sala %></td>
        <td><%= sala.co_local %></td>
        <td><%= sala.no_sala %></td>
        <td><%= sala.qt_capacidad %></td>
        <td><%= sala.description %></td>
        <td><%= link_to 'Show', sala %></td>
        <td><%= link_to 'Edit', edit_sala_path(sala) %></td>
        <td><%= link_to 'Destroy', sala, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sala', new_sala_path %>
