<%= simple_form_for(@sala) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :co_sala %>
    <%= f.input :co_local %>
    <%= f.input :no_sala %>
    <%= f.input :qt_capacidad %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
