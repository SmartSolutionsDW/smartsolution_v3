<p id="notice"><%= notice %></p>

<h1>Listing Reservas</h1>

<table>
  <thead>
    <tr>
      <th>Co reserva</th>
      <th>Co sala</th>
      <th>No dni</th>
      <th>Fe reserva</th>
      <th>Tx description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reservas.each do |reserva| %>
      <tr>
        <td><%= reserva.co_reserva %></td>
        <td><%= reserva.co_sala %></td>
        <td><%= reserva.no_dni %></td>
        <td><%= reserva.fe_reserva %></td>
        <td><%= reserva.tx_Description %></td>
        <td><%= link_to 'Show', reserva %></td>
        <td><%= link_to 'Edit', edit_reserva_path(reserva) %></td>
        <td><%= link_to 'Destroy', reserva, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reserva', new_reserva_path %>
