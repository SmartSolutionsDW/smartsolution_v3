<%- model_class = Evento -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:local_id) %>:</strong></dt>
  <dd><%= @evento.local_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fe_evento) %>:</strong></dt>
  <dd><%= @evento.fe_evento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:capacidad) %>:</strong></dt>
  <dd><%= @evento.capacidad %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @evento.descripcion %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              eventos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_evento_path(@evento), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              evento_path(@evento),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
