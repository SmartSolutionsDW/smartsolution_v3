<%= simple_form_for(@evento) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :co_evento %>
    <%= f.input :co_local %>
    <%= f.input :fe_evento %>
    <%= f.input :qt_capacidad %>
    <%= f.input :descripcion %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
